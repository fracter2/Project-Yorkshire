script/source = "extends Panel
@onready var system_style:StyleBoxTexture = preload(\"res://src/Menus/Main Menu.tscn::StyleBoxTexture_c6fuk\")

# Called when the node enters the scene tree for the first time.
func _ready():
	PauseMenu.in_main_menu = true
	Window.new()
	get_window().min_size = Vector2i(640,640)
	
	
	
	
	system_style.texture.gradient.colors[0] = DisplayServer.get_accent_color()
	system_style.texture.gradient.colors[1] = DisplayServer.get_accent_color().inverted().darkened(0.6)
	add_theme_stylebox_override(\"System Accent Color\", system_style)


# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(delta):
	pass
"

